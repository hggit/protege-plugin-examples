package com.similar2.matcher.ontology.model.impl;

import com.similar2.matcher.ontology.model.IQuantitativeProbability;

/**
 * 
 * @author Anthony
 */
public class QuantitiveProbability implements IQuantitativeProbability{

	/**
	 * the probability in term of a real number
	 */
	private Float probability; 
	
	public QuantitiveProbability(){}
	
	public QuantitiveProbability(Float probability){
		this.probability = probability;
	}

	/* (non-Javadoc)
	 * @see com.similar2.matcher.ontology.model.IQuantitiveProbability#getProbability()
	 */
	@Override
	public Float getProbability() {
		return this.probability;
	}

	/* (non-Javadoc)
	 * @see com.similar2.matcher.ontology.model.IQuantitiveProbability#setProbability(java.lang.Float)
	 */
	@Override
	public void setProbability(Float probability) {
		this.probability = probability;
	}
	
}